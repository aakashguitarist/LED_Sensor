void setup() {

  // put your setup code here, to run once:

pinMode(A0,INPUT);

pinMode(12,OUTPUT);

Serial.begin(9600);

}

 

void loop() {

  // put your main code here, to run repeatedly:

int a=analogRead(A0);

Serial.println(a);

if(a<1000){

  digitalWrite(12,HIGH);

}

else{

  digitalWrite(12,LOW);

}

}

 
